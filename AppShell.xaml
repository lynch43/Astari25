<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Astari25.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Astari25"
    xmlns:views="clr-namespace:Astari25.Views"
    Title="Astari25">

    <ShellContent
        Title="Start"
        ContentTemplate="{DataTemplate views:StartPage}"/>
    <ShellContent
    Title="Game"
    ContentTemplate="{DataTemplate views:GamePage}"/>
    <ShellContent
    Title="Settings"
    ContentTemplate="{DataTemplate views:SettingsPage}"/>

</Shell>
